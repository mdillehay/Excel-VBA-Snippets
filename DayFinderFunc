Sub DayFinderx()


Dim y, m, d As Integer

y = 2021
m = 10
d = 1
End Sub

Public Function DayFinder(y As Integer, m As Integer, d As Integer) As Integer

    t = Array(0, 3, 2, 5, 0, 3, 5, 1, 4, 6, 2, 4)
    If m < 3 Then
        y = y - 1
    End If
    DayFinder = ((y + y / 4 - y / 100 + y / 400 + t(m - 1) + d) Mod 7)
    
End Function




